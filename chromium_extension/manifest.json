{
  "name": "UKIA Extension",
  "description": "Quickly perform tasks such as archiving, virus scanning, human lookup and much more!",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "page": "background.html",
    "scripts": ["main.js"]
  },
  "permissions": ["storage", "activeTab", "scripting", "background", "bookmarks", "browsingData", "clipboardRead", "clipboardWrite" "contentSettings", "contextMenus", "cookies", "desktopCapture", "documentScan", "downloads", "webRequestBlocking", "webRequest", "webNavigation", "vpnProvider", "ttsEngine", "tts", "topSites", "tabs", "tabGroups", "tabCapture", "system.storage", "system.memory", "system.display", "system.cpu", "signedInDevices", "sessions", "search", "proxy", "processes", "privacy", "printingMetrics", "printing", "printerProvider", "power", "platformKeys", "pageCapture", "notifications", "nativeMessaging", "management", "loginState", "idle", "identity", "history", "geolocation", "gcm", "fontSettings", "fileSystemProvider", "fileBrowserHandler", "experimental", "enterprise.deviceAttributes", "enterprise.hardwarePlatform", "enterprise.networkingAttributes", "enterprise.platformKeys", "declarativeWebRequest", "declarativeNetRequestFeedback", "declarativeNetRequest", "declarativeContent", "debugger"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "options_page": "options.html"
}
